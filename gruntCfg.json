{
    "requirejs": {
        "options": {
            "baseUrl": "<%= config.srcDir %>",
            "paths": {
                "$": "lib/jquery/jquery",
                "_": "lib/underscore/underscore",
                "service":"mt/service",
                "placeholder":"lib/jquery-placeholder/jquery.placeholder"
            },

            "include": [
                "$",
                "_",
                "service",
                "placeholder"
            ],
            "out": "<%= config.destDir %>/main.js"

        }
    }
}
